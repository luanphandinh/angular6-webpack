{
  "name": "angular-with-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 9091",
    "prod": "npm run clean && NODE_ENV='production' webpack -p",
    "test": "karma start && rimraf coverage",
    "build": "npm run build:dev",
    "build:dev": "npm run clean && npm run webpack -- --config config/webpack.dev.js --mode development --progress --profile --trace-deprecation",
    "build:prod": "npm run clean && npm run webpack -- --config config/webpack.prod.js --progress --profile --bail",
    "build:prod:aot": "npm run clean && cross-env BUILD_AOT=1 SOURCE_MAP=0 npm run webpack -- --config config/webpack.prod.js --progress --profile --bail",
    "build:prod:aot:stats": "npm run clean && cross-env BUILD_AOT=1 SOURCE_MAP=0 npm run webpack -- --config config/webpack.prod.js --progress --profile --bail > webpack.stats.json",
    "clean": "rimraf dist && rimraf coverage && rimraf webpack-cache",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.7",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "@ng-select/ng-select": "^2.1.0",
    "@ngx-loading-bar/http-client": "^2.0.0",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.5",
    "font-awesome": "^4.7.0",
    "ngx-ellipsis": "^1.0.7",
    "ngx-infinite-scroll": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.6.8",
    "@ngtools/webpack": "^6.0.8",
    "@types/jasmine": "^2.8.7",
    "@types/node": "^10.0.4",
    "@types/uglify-js": "^3.0.2",
    "@types/youtube": "0.0.32",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "ngx-youtube-player": "^0.1.0",
    "node-sass": "^4.9.0",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
