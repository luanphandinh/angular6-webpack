    "tslint-config-airbnb": "^5.8.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.4",
    "webpack-inline-manifest-plugin": "^4.0.1",
    "webpack-merge": "^4.1.2"
  }
}
